<div id="song-<%= @song.id %>">
  <%= @toolbar  %>
  <div class="vertical-scroll">
    <div>
      <ul class="rounded">
        <li><small>Name</small><%=@song.name%></li>
        <li><small>Nickname</small><%=@song.pt_nickname%></li>
        <li>Original: <%= @song.original? ? "YES": "NO" %></li>
        <li>Author: <%=@song.author%></li>
      </ul>
    </div>
  </div>
</div>