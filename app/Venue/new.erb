<div class="toolbar">
	<h1 id="pageTitle">
		New Venue
	</h1>
</div>

<form title="New Venue" 
	  class="panel"
	  id="venue_new_form" 
	  method="POST" 
	  action="<%=url_for(:action => :create)%>" selected="true">
	<fieldset>
		<input type="hidden" name="id" value="<%=@venue.object%>"/>
		
		  <div class="row">
			  <label>City: </label>
			  <input type="text" name="venue[city]"/>
		  </div>
		
		  <div class="row">
			  <label>Lat: </label>
			  <input type="text" name="venue[lat]"/>
		  </div>
		
		  <div class="row">
			  <label>Lng: </label>
			  <input type="text" name="venue[lng]"/>
		  </div>
		
		  <div class="row">
			  <label>Name: </label>
			  <input type="text" name="venue[name]"/>
		  </div>
		
		  <div class="row">
			  <label>Pt: </label>
			  <input type="text" name="venue[pt_id]"/>
		  </div>
		
		  <div class="row">
			  <label>State: </label>
			  <input type="text" name="venue[state]"/>
		  </div>
		
		  <div class="row">
			  <label>Created at: </label>
			  <input type="text" name="venue[created_at]"/>
		  </div>
		
		  <div class="row">
			  <label>Updated at: </label>
			  <input type="text" name="venue[updated_at]"/>
		  </div>
		
	</fieldset>
	<input type="submit" value="Create"/>
</form>