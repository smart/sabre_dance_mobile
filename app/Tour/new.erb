<div class="toolbar">
	<h1 id="pageTitle">
		New Tour
	</h1>
</div>

<form title="New Tour" 
	  class="panel"
	  id="tour_new_form" 
	  method="POST" 
	  action="<%=url_for(:action => :create)%>" selected="true">
	<fieldset>
		<input type="hidden" name="id" value="<%=@tour.object%>"/>
		
		  <div class="row">
			  <label>Name: </label>
			  <input type="text" name="tour[name]"/>
		  </div>
		
		  <div class="row">
			  <label>Pt: </label>
			  <input type="text" name="tour[pt_id]"/>
		  </div>
		
		  <div class="row">
			  <label>Created at: </label>
			  <input type="text" name="tour[created_at]"/>
		  </div>
		
		  <div class="row">
			  <label>Updated at: </label>
			  <input type="text" name="tour[updated_at]"/>
		  </div>
		
	</fieldset>
	<input type="submit" value="Create"/>
</form>