<div id="shows-years">
  <%= @toolbar  %>
  <div class="vertical-scroll">
    <div>
      <ul class="rounded">
        <% (1995..Date.today.year).to_a.reverse.each do |year| %>
          <li class="forward"><a href="/app/Show/year/{<%= year %>}"><%= year %></a></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>